{
  "name": "document-management-system",
  "version": "1.0.0",
  "description": "This repo contains an implementation of a full-stack document management system",
  "main": "server.js",
  "scripts": {
    "test": "npm-run-all seed:test test:server reset:test",
    "start": "babel-node server.js",
    "e2e": "nightwatch",
    "test:client": "jest",
    "test:client:watch": "npm test -- --watch",
    "db:migrate": "sequelize db:migrate",
    "seed": "NODE_ENV=development babel-node server/test/TestHelpers/SeedDatabaseData.js",
    "reset:test": "NODE_ENV=test babel-node server/test/TestHelpers/ResetDatabase.js",
    "seed:test": "NODE_ENV=test babel-node server/test/TestHelpers/SeedDatabaseData.js",
    "test:server": "NODE_ENV=test babel-node ./node_modules/babel-istanbul/lib/cli.js cover ./node_modules/.bin/_mocha server/test/**/*.spec.js && gulp coveralls",
    "build:dist": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./server/test,./client/test,./reports,./migrations,./coverage,docs --copy-files",
    "document": "rimraf docs/ && jsdoc -r -c ./conf.json --readme ./documentation/README.md --destination docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-tAdedotun/document-management-system.git"
  },
  "author": "",
  "license": "ISC",
